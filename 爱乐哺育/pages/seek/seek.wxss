/* pages/seek/seek.wxss */
.seek{
  width: 740rpx;
  margin-left: 4rpx;
  margin-top: 10rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
}
.seek_input{
  width: 73%;
  height: 100rpx;
  margin-left: 10rpx;
  color: #ADADAD;
  border: 1rpx solid #ADADAD;
  font-size: 32rpx;
}
.seek_botton{
  width: 22%;
  height: 100rpx;
  border: 1rpx solid #27A7CC;
  text-align: center;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #27A7CC;
  margin-left: 3%;
}
.xs{
  width: 650rpx;
  flex-wrap: wrap;
  margin-left: 30rpx;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #EFEFEF;
  color: #B4B4B4;
  font-size: 30rpx;
}
.kuang{
  width: 700rpx;
   border-bottom: 1rpx solid #9D9D9D;
  margin-left: 24rpx;
  margin-top: 10rpx;
}
.xians{
  width: 100%;
  height: 100rpx;
  margin-top: 20rpx;
}
.tel{
  width: 100%;
  flex-wrap: wrap;
  height: 50rpx;
  font-size: 36rpx;
  line-height: 50rpx;
  text-overflow: -o-ellipsis-lastline;  
  overflow: hidden;  
  text-overflow: ellipsis;  
  display: -webkit-box;  
  -webkit-line-clamp: 1;  
  -webkit-box-orient: vertical;
}
.type{
  width: 100%;
  height: 50rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #9D9D9D;
}